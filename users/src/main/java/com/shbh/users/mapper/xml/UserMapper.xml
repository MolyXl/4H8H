<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shbh.sentinel.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.shbh.sentinel.entity.User">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="pwd" property="pwd" />
        <result column="reg_time" property="regTime" />
        <result column="last_login_time" property="lastLoginTime" />
        <result column="id_card" property="idCard" />
        <result column="phone_no" property="phoneNo" />
        <result column="email_address" property="emailAddress" />
        <result column="gender" property="gender" />
        <result column="status" property="status" />
        <result column="remark" property="remark" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, pwd, reg_time, last_login_time, id_card, phone_no, email_address, gender, status, remark
    </sql>

</mapper>
